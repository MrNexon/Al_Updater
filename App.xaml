             xmlns:local="clr-namespace:Al_Updater"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="Transparent_Color">#01000000</Color>
        <Color x:Key="Opacity_Dark_Black_Color">#CC000000</Color>
        <Color x:Key="Opacity_Light_Black_Color">#99000000</Color>
